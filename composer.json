{
  "name": "popov/zfc-data-grid-plugin",
  "description": "ZF module manager which extend possibility of ZfcDatagrid",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "grid",
    "zfc-datagrid",
    "zfc-datagrid factory",
    "agere"
  ],
  "authors": [
    {
      "name": "Serhii Popov",
      "email": "popow.serhii@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "Popov\\ZfcDataGridPlugin\\": "src/",
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PopovTest\\ZfcDataGridPlugin\\": "test/"
    }
  }
  "extra": {
    "zf": {
	  "component": "Popov\\ZfcDataGridPlugin",
      "config-provider": "Popov\\ZfcDataGridPlugin\\ConfigProvider"
    }
  }
}
